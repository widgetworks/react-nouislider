{
  "name": "@wiwo/nouislider-react",
  "private": false,
  "version": "3.4.2",
  "description": "React component wrapping leongersen/noUiSlider",
  "main": "dist/nouislider-react.cjs.js",
  "module": "dist/nouislider-react.esm.js",
  "browser": "dist/nouislider-react.umd.production.min.js",
  "typings": "index.d.ts",
  "sideEffects": false,
  "scripts": {
    "clear": "rm -rf dist",
    "build": "cross-env NODE_ENV=development rollup -c && cross-env NODE_ENV=production rollup -c",
    "build:dist": "npm run clear && npm run build",
    "test": "jest ./tests/",
    "test:coverage": "jest --coverage ./tests/",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "test:watch": "jest --watch ./tests/",
    "prepublishOnly": "npm run build",
    "precommit": "lint-staged"
  },
  "files": [
    "dist",
    "index.d.ts",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mmarkelov/react-nouislider"
  },
  "keywords": [
    "react",
    "nouislider",
    "slider",
    "component",
    "reactjs",
    "range",
    "slider"
  ],
  "author": "Maksim Markelov <mapkeji26@gmail.com>",
  "homepage": "https://github.com/mmarkelov/react-nouislider",
  "license": "MIT",
  "dependencies": {
    "nouislider": ">= 11.x",
    "prop-types": "^15.7.2"
  },
  "peerDependencies": {
    "nouislider": ">= 11.x",
    "react": ">= 16.8.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.8.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.1.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "enzyme": "^3.11.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "jest": "^27.1.0",
    "jest-environment-jsdom": "^27.5.1",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.56.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^7.0.2"
  }
}
